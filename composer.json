{
    "name": "dotdefine/openweather",
    "description": "Integrate weather information from any location right into your Craft CMS 3 website",
    "type": "craft-plugin",
    "version": "1.0.6",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "weather"
    ],
    "support": {
        "docs": "https://github.com/dotdefine/openweather/blob/master/README.md",
        "issues": "https://github.com/dotdefine/openweather/issues"
    },
    "license": "MIT",
    "authors": [{
        "name": "dotdefine forked by Bolden B.V.",
        "homepage": "https://www.dotdefine.com"
    }],
    "require": {
        "craftcms/cms": "^3.0.0"
    },
    "autoload": {
        "psr-4": {
            "dotdefine\\openweather\\": "src/"
        }
    },
    "extra": {
        "name": "OpenWeather",
        "handle": "openweather",
        "schemaVersion": "1.0.0",
        "hasCpSettings": true,
        "hasCpSection": false,
        "class": "dotdefine\\openweather\\OpenWeather"
    }
}
